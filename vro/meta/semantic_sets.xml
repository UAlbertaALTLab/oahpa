<?xml version="1.0" encoding="utf-8"?>
<!-- Specify subsets for sematic supersets here -->
<lexicon>
	<subclasses class="HUMAN">  <!--Contains not HUMAN-LIKE nor ACTOR, because ACTOR does not fit as answer to "Who do you see?"  -->
      <sem class="PROFESSION"/>   
      <sem class="ACTOR"/>
      <sem class="OTHERS"/>
    </subclasses>
	<subclasses class="HUMAN-LIKE">  
      <sem class="MYTH_HUM"/>  
      <sem class="HUM_V"/>  
      <sem class="HUM_A"/>   <!-- HUMAN_A is meant only for Morfa, and contains words from other sets also -->
      <sem class="VERBAL_V"/>   
      <sem class="EXPERIENCE_V"/>       
      <sem class="AUX_V"/> <!-- Verbs which typically are followed by an Inf -->
    </subclasses>
    <subclasses class="MYTH">  <!-- Morfa-set, for Essive -->
      <sem class="MYTH_HUM"/>  
      <sem class="MYTH_OTHER"/>  
    </subclasses>
    <subclasses class="ANIMAL">  
      <sem class="ANIMAL_DOM"/>  <!-- domestic --> 
      <sem class="ANIMAL_WILD"/>
      <sem class="ANIMAL_PET"/>
      <sem class="ANIMAL_OTHER"/>
      <sem class="BIRD"/> <!-- can fly, also insects -->
      <sem class="FISH"/> <!-- living in water, so it contains also some animals --> 
      <sem class="ANIMAL_V"/> <!-- what animals do and we do to animals -->
      <sem class="ANIMAL_A"/>
    </subclasses>
    <subclasses class="FOOD/DRINK">  
      <sem class="FOOD_GROCERY"/>  <!-- one can buy in the shop -->
