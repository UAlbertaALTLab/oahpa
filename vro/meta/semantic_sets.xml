      <sem class="FOOD_OTHER"/>
      <sem class="DRINK"/>  
      <sem class="FOOD_DRINK_V"/> 
      <sem class="SOUP_A"/> <!-- adjectives about food -->
    </subclasses>
    <subclasses class="TIME">   
