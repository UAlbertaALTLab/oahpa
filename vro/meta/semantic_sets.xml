      <sem class="TIME_POINT_GEN"/> <!-- it happened then gen, e.g. mánnodat -->
      <sem class="TIME_SEASON"/>  <!--  e.g. čakča -->
      <sem class="TIME_POINT_GEN_PL"/> <!-- it happened then gen-pl, e.g. juovllat  -->
      <sem class="TIME_OTHER"/>
      <sem class="TIME_ADV"/>
    </subclasses>
    <subclasses class="PLACE">  <!-- Fit as answer to "Where is the girl?"  -->
