SELECT:r1000 N IF (-1 (A Attr) OR Indef)(NEGATE 0 Inf OR VFIN OR Der/veara OR ("veara") OR ("leat" PrfPrc) OR QDL)(NEGATE -1 Adv)(1 NP-BOUNDARY);  
